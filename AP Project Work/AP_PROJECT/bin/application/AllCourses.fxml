<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="540.0" prefWidth="717.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <Label layoutX="45.0" prefHeight="134.0" prefWidth="801.0" text="All Courses " textFill="#1834d6">
         <font>
            <Font name="Bodoni MT" size="80.0" />
         </font>
      </Label>
      <VBox layoutX="1.0" layoutY="141.0">
         <children>
            <TableView fx:id="tableView" editable="true" prefHeight="386.0" prefWidth="937.0" VBox.vgrow="ALWAYS">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
      <Button fx:id="mycoursesback" layoutX="818.0" layoutY="67.0" mnemonicParsing="false" onAction="#mycoursesback" prefHeight="17.0" prefWidth="86.0" text="Back">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Button>
      <Button fx:id="jbtn1" layoutX="446.0" layoutY="108.0" mnemonicParsing="false" onAction="#course" text="View Courses" />
   </children>
</AnchorPane>
