<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="540.0" prefWidth="893.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <Label layoutX="98.0" layoutY="20.0" prefHeight="134.0" prefWidth="638.0" text="Student Requests" textFill="#2c0303">
         <font>
            <Font name="Colonna MT" size="81.0" />
         </font>
      </Label>
      <Button fx:id="goback" layoutX="748.0" layoutY="37.0" mnemonicParsing="false" onAction="#gotolast" prefHeight="17.0" prefWidth="86.0" text="Back">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Button>
      <VBox layoutX="49.0" layoutY="203.0" prefHeight="249.0" prefWidth="758.0">
         <children>
            <TableView fx:id="reqtable" prefHeight="250.0" prefWidth="811.0" />
         </children>
      </VBox>
      <Button fx:id="viewreq" layoutX="366.0" layoutY="163.0" mnemonicParsing="false" onAction="#viewreq" prefHeight="26.0" prefWidth="125.0" text="View Requests" />
      <TextField fx:id="tbookk" layoutX="244.0" layoutY="475.0" prefHeight="42.0" prefWidth="192.0" promptText="Request ID">
         <font>
            <Font name="Verdana" size="15.0" />
         </font></TextField>
      <Button fx:id="bookk" layoutX="506.0" layoutY="475.0" mnemonicParsing="false" onAction="#bookk" prefHeight="42.0" prefWidth="79.0" text="Book">
         <font>
            <Font name="Verdana" size="18.0" />
         </font></Button>
      <Button fx:id="reject" layoutX="602.0" layoutY="475.0" mnemonicParsing="false" onAction="#reject" prefHeight="42.0" prefWidth="86.0" text="Reject">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
